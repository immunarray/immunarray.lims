<?xml version="1.0"?>
<object name="portal_catalog">
  <!-- Veracis Sample Serial number that is used as a running count of samples
  that have come into the lab, key lookup for samples pre LIMS-->
  <index name="sample_serial_number" meta_type="FieldIndex">
    <indexed_attr value="sample_serial_number"/>
  </index>
  <column value="sample_serial_number"/>
  <!-- Expiraiton data of iChipLot that is used as a fast look up for viability
  of iChipLots for commercial use-->
  <index name="ichip_lot_expiration_date" meta_type="FieldIndex">
    <indexed_attr value="ichip_lot_expiration_date"/>
  </index>
  <column value="ichip_lot_expiration_date"/>
  <!-- Running count of select run purpose sessions, want to have this on
  surface of objects to allow for ease of assignment for new test runs-->
  <index name="run_number" meta_type="FieldIndex">
    <indexed_attr value="run_number"/>
  </index>
  <column value="run_number"/>
  <!-- Reworking this as billing is evolving -->
  <index name="billable_code" meta_type="FieldIndex">
    <indexed_attr value="billable_code"/>
  </index>
  <column value="billable_code"/>
  <!-- Veracis ID is indexed and added to metadata, together these are
  used to create new sequential IDs. -->
  <index name="veracis_id" meta_type="FieldIndex">
    <indexed_attr value="veracis_id"/>
  </index>
  <column value="veracis_id"/>
  <!-- Remaining volume for overview of samples/aliquots -->
  <index name="remaining_volume" meta_type="FieldIndex">
    <indexed_attr value="remaining_volume"/>
  </index>
  <column value="remaining_volume"/>
  <column value="product_name"/>
  <!-- Box Numbers for aliquots that are in the inventory system -->
  <index name="box_number" meta_type="FieldIndex">
    <indexed_attr value="box_number"/>
  </index>
  <column value="box_number"/>
  <!-- source_id_one for RandDSample  -->
  <index name="source_id_one" meta_type="FieldIndex">
    <indexed_attr value="source_id_one"/>
  </index>
  <column value="source_id_one"/>
  <!-- source_id_two for RandDSample  -->
  <index name="source_id_two" meta_type="FieldIndex">
    <indexed_attr value="source_id_two"/>
  </index>
  <column value="source_id_two"/>
  <!-- source_id_three for RandDSample  -->
  <index name="source_id_three" meta_type="FieldIndex">
    <indexed_attr value="source_id_three"/>
  </index>
  <column value="source_id_three"/>
</object>
