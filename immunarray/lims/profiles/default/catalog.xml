<?xml version="1.0"?>
<object name="portal_catalog">
  <!-- Veracis Sample Serail number that is used as a running count of samples
  that have come into the lab, key lookup for samples pre LIMS-->
  <index name="sample_serial_number" meta_type="FieldIndex"/>
  <column value="sample_serial_number"/>
  <!-- Expiraiton data of iChipLot that is used as a fast look up for viability
  of iChipLots for commercial use-->
  <index name="ichip_lot_expiration_date" meta_type="FieldIndex"/>
  <column value="ichip_lot_expiration_date"/>
  <!-- Phase this out, moved to workflow! -->
  <index name="sample_status" meta_type="DateIndex"/>
  <column value="sample_status"/>
  <!-- Running count of select run purpose sessions, want to have this on
  surface of objects to allow for ease of assignment for new test runs-->
  <index name="veracis_run_number" meta_type="FieldIndex"/>
  <column value="veracis_run_number"/>
  <!-- Categorize the different test run types of the lab-->
  <index name="veracis_run_purpose" meta_type="FieldIndex"/>
  <column value="veracis_run_purpose"/>
  <!-- Running count of all test sessions, want to have this on surface of
  objects to allow for ease of assignment for new test runs-->
  <index name="veracis_run_serial_number" meta_type="FieldIndex"/>
  <column value="veracis_run_serial_number"/>
  <!-- Reworking this as billing is evolving -->
  <index name="billable_code" meta_type="FieldIndex"/>
  <column value="billable_code"/>
  <!-- Allow for sorting of iChips without opening the object-->
  <index name="ichip_status" meta_type="FieldIndex"/>
  <column value="ichip_status"/>
</object>
