  </bindings>

  <bindings>
    <type type_id="iChipLot">
      <bound-workflow workflow_id="ichiplot_workflow"/>
    </type>
  </bindings>

  <bindings>
    <type type_id="ClinicalSample">
      <bound-workflow workflow_id="clinicalsample_workflow"/>
    </type>
  </bindings>


</object>
