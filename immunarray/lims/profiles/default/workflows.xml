<?xml version="1.0"?>
<object name="portal_workflow" meta_type="Plone Workflow Tool" purge="False">
  <property name="title">Contains workflow definitions for your portal
  </property>

  <object name="ichiplot_workflow" meta_type="Workflow"/>
  <object name="ichip_workflow" meta_type="Workflow"/>
  <object name="clinicalsample_workflow" meta_type="Workflow"/>
  <object name="qcsample_workflow" meta_type="Workflow"/>
  <object name="qcaliquot_workflow" meta_type="Workflow"/>
  <object name="clinicalaliquot_workflow" meta_type="Workflow"/>
  <object name="material_workflow" meta_type="Workflow"/>
  <object name="assayrequest_workflow" meta_type="Workflow"/>
  <object name="billingprogram_workflow" meta_type="Workflow"/>
  <object name="assaybillingrequest_workflow" meta_type="Workflow"/>
  <object name="ichipassay_workflow" meta_type="Workflow"/>

  <bindings>

    <type type_id="iChip">
      <bound-workflow workflow_id="ichip_workflow"/>
    </type>

    <type type_id="iChipLot">
      <bound-workflow workflow_id="ichiplot_workflow"/>
    </type>

    <type type_id="ClinicalSample">
      <bound-workflow workflow_id="clinicalsample_workflow"/>
    </type>

    <type type_id="ClinicalAliquot">
      <bound-workflow workflow_id="clinicalaliquot_workflow"/>
    </type>

    <type type_id="QCSample">
      <bound-workflow workflow_id="qcsample_workflow"/>
    </type>

    <type type_id="QCAliquot">
      <bound-workflow workflow_id="qcaliquot_workflow"/>
    </type>

    <type type_id="Material">
      <bound-workflow workflow_id="material_workflow"/>
    </type>

    <type type_id="AssayRequest">
      <bound-workflow workflow_id="assayrequest_workflow"/>
    </type>

    <type type_id="BillingProgram">
      <bound-workflow workflow_id="billingprogram_workflow"/>
    </type>

    <type type_id="AssayBillingRequest">
      <bound-workflow workflow_id="assaybillingrequest_workflow"/>
    </type>

    <type type_id="iChipAssay">
      <bound-workflow workflow_id="ichipassay_workflow"/>
    </type>

  </bindings>

</object>
